{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://homesec-ui1.vercel.app/$1",
      "permanent": true
    }
  ]
} 